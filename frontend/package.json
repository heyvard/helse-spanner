{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "cd .. && npx husky install frontend/.husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --rule react-hooks/exhaustive-deps:off"
    ]
  },
  "dependencies": {
    "prettier": "^2.3.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-json-view": "^1.21.3",
    "react-query": "^3.23.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  }
}
